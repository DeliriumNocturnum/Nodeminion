<%
var count = 0;
for (i = 0; i < onUsers.length; i++) {
  
  if (count === 0) { %>
    <tr>
      <td><button id="<%-onUsers[i]._id%>-plbutton" status="<%-((onUsers[i].local.status === 1) ? "1" : ((onUsers[i].local.status === 2) ? "2": "0")) %>" onclick="addPlayer(this.value,'<%-onUsers[i].local.username%>','<%-((user)?user._id:false)%>','<%-((user)?user.local.username:false)%>')" <%- ((!user) ? 'disabled' : ((String(user._id) == String(onUsers[i]._id) || [1,2].indexOf(onUsers[i].local.status) >= 0 || [1,2].indexOf(user.local.status) >= 0) ? 'disabled' : '' ))%> class="player-button uk-button uk-button-mini" type="button" value="<%-onUsers[i]._id%>"><i id="<%-onUsers[i]._id%>-plicon" class="uk-icon-user" style="color:<%-((onUsers[i].local.status === 1) ? "#D7DF01" : ((onUsers[i].local.status === 2) ? "#B40404": "#31B404")) %>"></i> <%-onUsers[i].local.username%> </button></td>
  <% count += 1; } else if (count < 4 && count > 0) { %>
      <td><button id="<%-onUsers[i]._id%>-plbutton" status="<%-((onUsers[i].local.status === 1) ? "1" : ((onUsers[i].local.status === 2) ? "2": "0")) %>" onclick="addPlayer(this.value,'<%-onUsers[i].local.username%>','<%-((user)?user._id:false)%>','<%-((user)?user.local.username:false)%>')" <%- ((!user) ? 'disabled' : ((String(user._id) == String(onUsers[i]._id) || [1,2].indexOf(onUsers[i].local.status) >= 0 || [1,2].indexOf(user.local.status) >= 0) ? 'disabled' : '' ))%> class="player-button uk-button uk-button-mini" type="button" value="<%-onUsers[i]._id%>"><i id="<%-onUsers[i]._id%>-plicon" class="uk-icon-user" style="color:<%-((onUsers[i].local.status === 1) ? "#D7DF01" : ((onUsers[i].local.status === 2) ? "#B40404": "#31B404")) %>"></i> <%-onUsers[i].local.username%> </button></td>
  <% count += 1; } else { %>
      <td><button id="<%-onUsers[i]._id%>-plbutton" status="<%-((onUsers[i].local.status === 1) ? "1" : ((onUsers[i].local.status === 2) ? "2": "0")) %>" onclick="addPlayer(this.value,'<%-onUsers[i].local.username%>','<%-((user)?user._id:false)%>','<%-((user)?user.local.username:false)%>')" <%- ((!user) ? 'disabled' : ((String(user._id) == String(onUsers[i]._id) || [1,2].indexOf(onUsers[i].local.status) >= 0 || [1,2].indexOf(user.local.status) >= 0) ? 'disabled' : '' ))%> class="player-button uk-button uk-button-mini" type="button" value="<%-onUsers[i]._id%>"><i id="<%-onUsers[i]._id%>-plicon" class="uk-icon-user" style="color:<%-((onUsers[i].local.status === 1) ? "#D7DF01" : ((onUsers[i].local.status === 2) ? "#B40404": "#31B404")) %>"></i> <%-onUsers[i].local.username%> </button></td>
      </tr>
  <% count = 0; };
}
%>

